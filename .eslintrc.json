{
    "extends": [
        "prettier",
        "prettier/standard",
        "eslint:recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    }
}
